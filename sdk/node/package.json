{
  "name": "@hpkv/websocket-client",
  "version": "1.2.13",
  "description": "HPKV WebSocket client for Node.js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --module commonjs --outDir dist/cjs && tsc --module esnext --outDir dist/esm --declaration false && tsc --module esnext --emitDeclarationOnly --declaration --outDir dist/types",
    "test": "jest --testTimeout=30000 --hookTimeout=30000",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "basic-example": "ts-node examples/basic-usage.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"examples/**/*.ts\"",
    "check": "npm run lint && npm run format:check && npm run test"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "hpkv",
    "websocket",
    "key-value store",
    "database",
    "key value store",
    "key value database",
    "client sdk",
    "high-performance key value",
    "pub-sub",
    "typescript"
  ],
  "author": "HPKV Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hpkv-io/websocket-client"
  },
  "bugs": {
    "url": "https://github.com/hpkv-io/websocket-client/issues"
  },
  "homepage": "https://github.com/hpkv-io/websocket-client/sdk/node#readme",
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "dependencies": {
    "@types/ws": "^8.5.10",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2"
  }
}
